    "i18next-http-middleware": "^3.2.2",
    "mysql2": "^3.0.0",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
